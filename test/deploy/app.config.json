{
	"services": [
		{
			"name": "history",
			"alias": "history",
			"dependencies": ["history-inmemory"]
		},
		{
			"name": "userlist",
			"alias": "userlist",
			"dependencies": ["user-list-inmemory"]
		}
	],
	"modules": [
		{
			"name": "history-inmemory",
			"file": "history.wasm",
			"config": {}
		},
		{
			"name": "user-list-inmemory",
			"file": "user-list.wasm",
			"config": {}
		}
	],
	"scripts": [
		{
			"name": "set_tetraplet",
			"file": "set_tetraplet.air",
			"variables": {
				"relay": "$relay",
				"node": "$node",
				"function": "is_authenticated",
				"json_path": "$.[\"is_authenticated\"]"
			}
		}
	],
	"script_storage": [
		{
			"name": "remove_disconnected",
			"file": "remove_disconnected.air",
			"interval": 10
		}
	]
}
